<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jonathan.androidfinalprojectjonathanprince" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.jonathan.androidfinalprojectjonathanprince.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.jonathan.androidfinalprojectjonathanprince.MovieDetails"
            android:label="@string/title_activity_movie_details"
            android:parentActivityName="com.example.jonathan.androidfinalprojectjonathanprince.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jonathan.androidfinalprojectjonathanprince.MainActivity" />
        </activity>
        <activity
            android:name="com.example.jonathan.androidfinalprojectjonathanprince.AddMovie"
            android:label="@string/title_activity_add_movie"
            android:parentActivityName="com.example.jonathan.androidfinalprojectjonathanprince.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jonathan.androidfinalprojectjonathanprince.MovieDetails" />
        </activity>

        <activity
            android:name="com.example.jonathan.androidfinalprojectjonathanprince.PlayMovie"
            android:label="@string/title_activity_play_movie"
            android:parentActivityName="com.example.jonathan.androidfinalprojectjonathanprince.MovieDetails" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jonathan.androidfinalprojectjonathanprince.MovieDetails" />
        </activity>
    </application>

</manifest>
